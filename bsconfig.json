{
  "name": "getml-jupyter-widgets",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core"
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "gentypeconfig": {
    "language": "typescript",
    "generatedFileExtension": ".gen.tsx",
    "module": "es6",
    "shims": {
      "ReasonReact": "ReactShim"
    }
  },
  "bsc-flags": [
    "-open RescriptCore"
  ]
}
